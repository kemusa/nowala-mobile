{
  "expo": {
    "name": "Nowala",
    "slug": "NowalaApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*", "assets/images/*"],
    "ios": {
      "buildNumber": "13",
      "supportsTablet": true,
      "bundleIdentifier": "io.nowala.sponsors"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#475972"
      },
      "package": "io.nowala.sponsors"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
